{
  "name": "twentytwentyfive-child-lucca",
  "version": "1.0.0",
  "description": "Lucca Gourmet WordPress Child Theme with Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./dist/tailwind.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./dist/tailwind.css --minify",
    "dev": "npm run build-css",
    "build": "npm run build-css-prod"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.16"
  },
  "author": "Lucca Gourmet",
  "license": "MIT"
}